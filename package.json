{
  "name": "leaflet.geojsonfilter",
  "version": "0.1.1",
  "description": "This extension of leaflet is inspired by mapbox setfilter function.",
  "main": "src/L.geoJsonFilter.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "browser-sync start --server --files \"**/*.html,**/*.css,**/*.js\"",
    "build-js": "cat  ./src/L.geoJsonFilter.js > ./dist/L.geojsonfilter-src.js",
    "compress": "uglifyjs ./dist/L.geojsonfilter-src.js -o ./dist/L.geojsonfilter.js -m",
    "build": "npm run build-js && npm run compress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonxiang/leaflet.geoJsonFilter.git"
  },
  "keywords": [
    "leaflet",
    "geojson",
    "filter"
  ],
  "author": "brandonxiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonxiang/leaflet.geoJsonFilter/issues"
  },
  "homepage": "https://github.com/brandonxiang/leaflet.geoJsonFilter#readme",
  "devDependencies": {
    "uglifyjs": "^2.4.10"
  }
}
